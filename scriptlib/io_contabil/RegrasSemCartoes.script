		}
		// Atualizar Chave caso encontrada.
	} else {
		try {
			dbIOCont.update("IO_EXTRATO")
			.fields("EXTRATOLIDO")
			.where("CHAVE = ? ")
			.param(0)
			.param(chaveExtrato)
			.execute();
		} catch(Exception e1) {
			logger.logError("Error updating IO_EXTRATO", e1);
		}
	}

	return true;

}


