    msg.append("<head>\n");
    msg.append("</head>\n");
    msg.append("<body lang=\"en-US\" dir=\"ltr\">\n");

    msg.append("<p><font style=\"font-size: 10pt\"></font>Prezado cliente, </p>\n");
