    String subject          = "";
    String sendToName       = "";
    String emailResponsavel = "";
    String tipoIntegracao   = "";
    String nomeArquivo      = "";
    String nomeEmpresa      = "";

    if(jsNaoExiste.has("SUBJECT")) subject = jsNaoExiste.optString("SUBJECT");
