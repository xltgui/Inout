            continue;
        }

        if (line.contains("MOVIMENTACAO") && line.contains("APLICACOES") && line.contains("RESGATES")) {
            fazExtra = true;
            continue;
        }
        logger.logDebug("MF0 " + memFile.getStringFieldRemoveEspCharsUpper(0));
