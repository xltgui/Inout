                            .param(debitoCredito)
                            .param(0)
                            .execute();
                } catch (Exception sqlInsertException) {
                    logger.logError("Error inserting to IO_EXTRATO", sqlInsertException);
                    continue;
                }
                // Atualizar Chave caso encontrada.
            } else {
                try {
                    dbIOCont.update("IO_EXTRATO")
                            .fields("EXTRATOLIDO, HISTORICO")
                            .where("CHAVE = ? ")
                            .param(0)
                            .param(nomeOrigem)
                            .param(chaveExtrato)
