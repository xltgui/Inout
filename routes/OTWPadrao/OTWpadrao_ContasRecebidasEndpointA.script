public String trataValor(String valor) {
    valor = valor.replaceAll("[-\\s]", "");
    valor = valor.replaceAll(",", "\\.");
    while (quantityOf(valor, ".") > 1) {
        valor = valor.replaceFirst("\\.", "");
    }
    return valor;
}


