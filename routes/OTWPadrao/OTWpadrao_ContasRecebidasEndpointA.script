                            .param(documento)
                            .param(nomePortador)
                            .param(nomeOrigem)
                            .param(nomeArquivo.toUpperCase())
                            .param(complemento01)
                            .param(valorDocumento)
                            .param(debitoCredito)
                            .param(0)
                            .execute();
                } catch(Exception e) {
                    logger.logError("Error inserting to IO_EXTRATO", e);
                    continue;
                }
            } else {
                try {

                    dbIOCont.update("IO_EXTRATO")
                            .fields("EXTRATOLIDO, HISTORICO")
                            .where("CHAVE = ? ")
                            .param(0)
                            .param(nomeOrigem)
