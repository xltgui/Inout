                            .param(valorDocumento)
                            .param(debitoCredito)
                            .param(0)
                            .execute();
                } catch (Exception sqlInsertException) {
                    logger.logError("Error inserting to IO_EXTRATO", sqlInsertException);
                    continue;
                }
            } else {
                try {
                    dbIOCont.update("IO_EXTRATO")
                            .fields("EXTRATOLIDO")
                            .where("CHAVE = ? ")
                            .param(0)
                            .param(chaveExtrato)
