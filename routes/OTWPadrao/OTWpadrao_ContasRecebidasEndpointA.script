            if (memFile.getFieldsSize() == 1) {
                nomeOrigemBackup = nomeOrigem;
                nomeOrigem += memFile.getStringFieldRemoveEspCharsUpper(0);
                try {
                    dbIOCont.update("IO_EXTRATO")
                            .fields("HISTORICO")
                            .where("CHAVE = ? ")
                            .param(nomeOrigem)
                            .param(chaveExtrato)
                            .execute();
